@model IEnumerable<Group_Project.Models.ApplicationUser>
@{
    Layout = "_Layout";
}
<h1>Manage Users</h1>
<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
                <tr>
                    <td>@user.Email</td>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>
                        <a href="#">View</a> | <a href="#">Delete</a>
                    </td>
                </tr>
        }
    </tbody>
</table>
